@page
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
    Layout = "~/Views/Shared/Layouts/_LayoutIdentity.cshtml";
}

<body>
    <div style="height:600px;">
        <h1 class="text-danger">@ViewData["Title"]</h1>
        <p class="text-danger">This account has been locked out, please try again after one hour or contact us.</p>
        <div class="mt-5">
            <a class="text-dark" asp-area="">Back to Home</a>
        </div>
    </div>
</body>
